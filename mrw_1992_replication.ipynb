    "axs[0].set_ylabel('Growth Rate: 1960 - 1985')\n",
    "axs[0].set_title('A: Unconditional')\n",
    "\n",
    "# Plot 2\n",
    "axs[1].scatter(res_c + np.mean(df2['log_Y60']),\n",
