    "res_h = residuals(df2['log_Y60'], df2[['log_s', 'log_ngd', 'log_school']])\n",
    "res_p3 = residuals(df2['log_diff'], df2[['log_s', 'log_ngd', 'log_school']])\n",
    "\n",
    "fig, axs = plt.subplots(3, 1, figsize=(8, 8), sharex='col')\n",
