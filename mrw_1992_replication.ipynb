    "axs[1].set_xlabel('Log Output Per Working Age Adult: 1960')\n",
    "axs[1].set_ylabel('Growth Rate: 1960 - 1985')\n",
    "axs[1].set_title('B: Conditional on Saving and Population Growth')\n",
